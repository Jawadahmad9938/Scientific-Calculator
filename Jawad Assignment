#include<iostream>
using namespace std;
#include<cmath>

int mysum(int a, int b){
	return a+b;
}
int mysub(int a,int b){
	return a-b;
}
int myprod(int a, int b){
	return a*b;
}
float mydiv(float a, float b){
	return a/b;
}

int main(){
	int a,b, add,sub,prod,div, quotient, remainder;
    cin>>a;
    cin>>b;
	add = mysum(a,b);
	cout<<"Addition is "<<add<<endl;
	
	sub = mysub(a,b);
	cout<<"Subtraction is "<<sub<<endl;
	
	prod = myprod(a,b);
	cout<<"Product is "<<prod<<endl;
	
	div = mydiv(a,b);
	cout<<"Division is "<<div<<endl;
	
	cout<<"log of "<<a<<" is "<<log(a)<<endl;
	cout<<"log of "<<b<<" is "<<log(b)<<endl;
	
	cout<<"SquareRoot of "<<a<<" is "<<sqrt(a)<<endl;
	cout<<"SquareRoot of "<<b<<" is "<<sqrt(b)<<endl;
	
	quotient = a / b;
	remainder = a % b;
	
	cout<<"Quotient of "<<a<<" and "<<b<<" is "<<quotient<<endl;
	cout<<"remainder of "<<a<<" and "<<b<<" is "<<remainder<<endl;
	
	cout<<"sin of "<<a<<" is "<<sin(a)<<endl;
	cout<<"sinh of "<<a<<" is "<<sinh(a)<<endl;
	
	cout<<"sin of "<<b<<" is "<<sin(b)<<endl;
	cout<<"sinh of "<<b<<" is "<<sinh(b)<<endl;
	
	cout<<"tan of "<<a<<" is "<<tan(a)<<endl;
	cout<<"tanh of "<<a<<" is "<<tanh(a)<<endl;
	
	cout<<"tan of "<<b<<" is "<<tan(b)<<endl;
	cout<<"tanh of "<<b<<" is "<<tanh(b)<<endl;
}
